{
  "name": "link",
  "type": "module",
  "version": "1.0.0",
  "main": "dist/main.js",
  "repository": "https://github.com/osuplace/link",
  "author": "April <april@dummy.cafe>",
  "license": "AGPL-3.0-or-later",
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "@auth/core": "0.25.0",
    "@auth/express": "0.5.0",
    "@auth/prisma-adapter": "1.2.0",
    "@prisma/client": "^5.11.0",
    "@types/ejs": "^3.1.5",
    "discord-api-types": "^0.37.74",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "prisma": "^5.11.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "register": "node dist/script/register.js",
    "db:push": "dotenv -e .env -c -- prisma db push",
    "db:migrate:dev": "dotenv -e .env -c -- prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env -c -- prisma migrate deploy",
    "db:generate": "dotenv -e .env -c -- prisma generate",
    "db:studio": "dotenv -e .env -c -- prisma studio"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.28",
    "typescript": "^5.4.2"
  },
  "imports": {
    "#link/*": "./dist/*"
  }
}
